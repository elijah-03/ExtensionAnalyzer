{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [{
      "css": ["content.css"],
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_idle"
   }],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com https://www.googletagmanager.com https://*.googleapis.com; object-src 'self'",
   "browser_action": {
      "default_icon": {
         "128": "icon-128.png",
         "48": "icon-48.png",
         "16": "icon-16.png"
      },
      "default_popup": "popup/index.html",
      "default_title": "__MSG_app_name__"
   },
   "default_locale": "en",
   "description": "__MSG_app_desc__",
   "icons": {
      "128": "icon-128.png",
      "48": "icon-48.png",
      "16": "icon-16.png"
   },
   "manifest_version": 2,
   "name": "__MSG_app_name__",
   "permissions": [ "tabCapture", "tabs", "storage", "\u003Call_urls>" ],
   "version": "2.35",
   "web_accessible_resources": [ "*.css" ]
}
