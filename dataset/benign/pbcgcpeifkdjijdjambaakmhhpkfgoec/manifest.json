      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  }
}