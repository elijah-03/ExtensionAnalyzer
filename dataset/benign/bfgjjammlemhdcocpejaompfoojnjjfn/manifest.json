{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PrinterLogic Extension v1.0.6.1",
  "short_name": "PrinterLogic Extension",
  "description": "Install and manage printers and print jobs using PrinterLogic.",
  "version": "1.0.6.1",
  "action":{
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "event.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ]
}
